{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": "./src",
    "paths": {
      "@styles/*": [
        "styles/*"
      ],
      "@styles": [
        "styles/index"
      ],
      "@interfaces": [
        "interfaces/index"
      ],
      "@interfaces/*": [
        "interfaces/*"
      ],
      "@actions": [
        "redux/actions/index"
      ],
      "@actions/*": [
        "redux/actions/*"
      ],
      "@reducers": [
        "redux/reducers/index"
      ],
      "@reducers/*": [
        "redux/reducers/*"
      ],
      "@store": [
        "redux/store"
      ],
      "@store/*": [
        "redux/*"
      ],
      "@services": [
        "services/firebaseClient"
      ],
      "@services/*": [
        "services/*"
      ],
      "@hooks": [
        "hooks/index"
      ],
      "@hooks/*": [
        "hooks/*"
      ],
      "@constants": [
        "constants/index"
      ],
      "@constants/*": [
        "constants/*"
      ],
      "@icons": [
        "components/icons/index"
      ],
      "@icons/*": [
        "components/icons/*"
      ],
      "@storage": [
        "services/storage/index"
      ],
      "@storage/*": [
        "services/storage/*"
      ],
      "@firestore": [
        "services/firestore/index"
      ],
      "@firestore/*": [
        "services/firestore/*"
      ],
      "@utils": [
        "utils/index"
      ],
      "@utils/*": [
        "utils/*"
      ],
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}